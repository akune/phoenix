<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  <g:HTMLPanel ui:field="conversationEntryContainer" addStyleNames="conversation-entry">
    <g:Image ui:field="avatarImage" addStyleNames="img-circle" url="img/avatar.png" />
    <g:InlineHTML ui:field="title" addStyleNames="title">Unknown</g:InlineHTML>
    <g:InlineHTML ui:field="closeConversationClickArea" addStyleNames="glyphicon glyphicon-remove-circle close-conversation" />
    <g:InlineHTML ui:field="unreadMessagesBadge" addStyleNames="badge">1</g:InlineHTML>
  </g:HTMLPanel>
</ui:UiBinder>
